<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gotogoal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".AchievementsActivity"
            android:label="Achievements" />
        <activity
            android:name=".WorkoutTrackActivity"
            android:label="Workout tracking"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".ExercisesActivity"
            android:label="Exercises" />
        <activity
            android:name=".MusclesActivity"
            android:label="Muscles" />
        <activity
            android:name=".BodyWeightActivity"
            android:label="Body weight" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>